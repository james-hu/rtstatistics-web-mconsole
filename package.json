{
  "name": "rtstatistics-web-mconsole",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/james-hu/rtstatistics-web-mconsole.git"
  },
  "scripts": {
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "start": "concurrent \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "postinstall": "typings install",
    "server": "tsc && webpack-dev-server --inline --progress --port 3000",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "test": "karma start"
  },
  "license": "Apache-2.0",
  "dependencies": {
